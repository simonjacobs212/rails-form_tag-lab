<h1>Student Form</h1>

<%= form_tag "/students" do %>
<%= label_tag 'First Name', 'First Name'%><br>
  <%= text_field_tag "first_name" %><br><br>
<%= label_tag 'Last Name', 'Last Name'%><br>
  <%= text_field_tag "last_name" %><br><br>
  <%= submit_tag "Submit Student" %>
<% end %>

<%@student.to_s%>